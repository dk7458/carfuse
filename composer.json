{
    "name": "carfuse/project",
    "description": "Car rental and management system with user management, payment handling, and advanced features.",
    "type": "project",
    "require": {
        "php": "^8.1",
        "firebase/php-jwt": "^6.11",
        "monolog/monolog": "^3.3",
        "vlucas/phpdotenv": "^5.5",
        "symfony/mailer": "^6.3",
        "symfony/http-foundation": "^6.3",
        "symfony/http-client": "^6.3",
        "ramsey/uuid": "^4.0",
        "nikic/fast-route": "^1.3",
        "guzzlehttp/guzzle": "^7.9",
        "phpmailer/phpmailer": "^6.9",
        "illuminate/http": "^10.48",
        "illuminate/support": "^10.48",
        "illuminate/events": "^10.48",
        "illuminate/database": "^10.38",
        "illuminate/console": "^10.48",
        "illuminate/filesystem": "^10.48",
        "nesbot/carbon": "^2.50.0",
        "php-di/php-di": "^7.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "AuditManager\\": "AuditManager/",
            "DocumentManager\\": "DocumentManager/",
            "Middleware\\": "App/Middleware/",
            "Services\\": "App/Services/",
            "Helpers\\": "App/Helpers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload -o",
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
